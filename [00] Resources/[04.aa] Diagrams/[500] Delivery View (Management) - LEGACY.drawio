        <mxCell id="QdbNLKzlp7Sa9JjDVJQk-16" value="Discovering &amp;amp; evolving the plan&amp;nbsp;" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="QdbNLKzlp7Sa9JjDVJQk-1">
