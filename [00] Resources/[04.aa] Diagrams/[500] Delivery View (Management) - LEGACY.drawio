        <mxCell id="QdbNLKzlp7Sa9JjDVJQk-20" value="&lt;p style=&quot;line-height: 90%&quot;&gt;Designing for Inputs&lt;/p&gt;" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="QdbNLKzlp7Sa9JjDVJQk-1">
