        </mxCell>
        <mxCell id="pzHZCPZMxvDHkfLBa03D-0" value="Digital Principles" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="rhp4sE3xphnoTrhWa3b3-1">
